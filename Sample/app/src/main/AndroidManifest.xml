<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.phunware.analytics.sample.App"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.phunware.analytics.sample.AnalyticsDemo"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.phunware.analytics.sample.SecondActivity"
            android:parentActivityName="com.phunware.analytics.sample.AnalyticsDemo" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.phunware.analytics.sample.AnalyticsDemo" />
        </activity>

        <meta-data
            android:name="com.phunware.maas.APPLICATION_ID"
            android:value="" />

        <meta-data
            android:name="com.phunware.maas.ACCESS_KEY"
            android:value="" />

    </application>

</manifest>